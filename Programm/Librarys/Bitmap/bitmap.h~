#ifndef BITMAP_H
#define BITMAP_H

#if defined(ARDUINO) && ARDUINO >= 100
#include "Arduino.h"
#else
#include "WProgram.h"
#endif  

class Bitmap
{
  public:
    void draw(char *filename, uint8_t x, uint16_t y);
};

#endif

